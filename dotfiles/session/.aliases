#!/bin/bash

export CODEBASE=$HOME/codebase
#
# CUSTOM
# 
alias github="cd ${CODEBASE}/github"
alias dl="cd ~/Downloads"
alias dc="docker-compose"
alias temp="cd ~/temp"
# 
# 
# ZSH
# 
alias zshconfig="vim ~/.zshrc"
# 
# IntelliJ
# 
INTELLIJ_VERSION_FOLDER=IdeaIC2024.3
alias ij-cache="cd ~/Library/Caches/JetBrains/${INTELLIJ_VERSION_FOLDER}"
